.unity-button {
    max-height: 20px;
    font-size: 8px;
    -unity-text-outline-color: rgba(56, 56, 56, 0);
    background-color: rgb(160, 255, 249);
}

.unity-button:hover {
    -unity-background-image-tint-color: rgb(255, 138, 138);
    background-color: rgb(217, 217, 217);
}

.unity-button:active {
    background-color: rgb(0, 0, 0);
}
